<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
		http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">

    <beans profile="child">
        <bean class="org.fujionclinical.epic.common.agl.AglService" />

        <bean parent="smartContextBase" depends-on="encounterContext"
              class="org.fujionclinical.epic.${fhir.context.version}.SmartContextEncounter" />

        <bean init-method="getInstance"
              class="org.fujionclinical.epic.common.servlet.MockEpicServiceRegistry" />
    </beans>
</beans>
